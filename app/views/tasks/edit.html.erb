<h1> Edit Your Task </h1>
<%= form_for @task do |f| %>
  <%= f.label :task %>
  <%= f.text_field :task %>
  <br>
  <br>
  <%= f.label :priority %>
  <%= f.select :priority, options_for_select([' ','1', '2', '3', '4', '5'])%>
  <br>
  <br>
  <%= f.label :completed %>
  <%= f.check_box :completed %>
  <br>
  <br>
  <%= f.submit %>
<% end %>

